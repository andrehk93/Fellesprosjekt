            <TextField fx:id="brukernavn" onKeyReleased="#enterKeyPress" prefHeight="41.0" prefWidth="269.0" promptText="E-post" />
            <Label fx:id="msg" prefHeight="30.0" prefWidth="500.0" styleClass="label-errormsg" GridPane.rowIndex="1">
               <font>
                  <Font name="Segoe UI Light" size="40.0" />
               </font>
            </Label>
            <Button fx:id="leggtil" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="33.0" prefWidth="343.0" styleClass="big-specialButton " text="Legg til i kalender" GridPane.rowIndex="2">
               <font>
                  <Font name="Garamond" size="18.0" />
               </font>
            </Button>
            <Button fx:id="removeCal" onAction="#handleButtonRemove" mnemonicParsing="false" prefHeight="33.0" prefWidth="343.0" styleClass="normalButton" text="Fjern ekstra kalendere" GridPane.rowIndex="4" />
