<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="300.0" maxWidth="400.0" minHeight="300.0" minWidth="400.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="klient.EPopController">
   <children>
      <GridPane alignment="CENTER_RIGHT" hgap="20.0" layoutX="45.0" layoutY="44.0" prefHeight="213.0" prefWidth="310.0" vgap="20.0">
